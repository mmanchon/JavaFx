<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.geometry.Insets?>
<?import javafx.geometry.Rectangle2D?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<VBox styleClass="vbox" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.Controllers.Teoria">
    <children>
        <Label alignment="CENTER" contentDisplay="CENTER" maxWidth="1.7976931348623157E308" prefHeight="21.0" text="C key words" textAlignment="CENTER" VBox.vgrow="ALWAYS" />
        <HBox styleClass="hbox" VBox.vgrow="ALWAYS">
            <children>
                <TextArea fx:id="explicacion" editable="false" wrapText="true" HBox.hgrow="SOMETIMES" />
                <ImageView fx:id="image" pickOnBounds="true" preserveRatio="true">
               <viewport>
                  <Rectangle2D />
               </viewport>
            </ImageView>
            </children>
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </padding>
        </HBox>
        <HBox prefHeight="0.0" prefWidth="439.0" styleClass="hbox">
            <children>
                <Button fx:id="seeMore" contentDisplay="RIGHT" mnemonicParsing="false" prefHeight="31.0" prefWidth="93.0" text="See more" />
                <Pane prefHeight="31.0" prefWidth="273.0" HBox.hgrow="ALWAYS" />
                <Button fx:id="accept" alignment="CENTER_RIGHT" contentDisplay="RIGHT" mnemonicParsing="false" onAction="#close" prefHeight="31.0" prefWidth="70.0" text="Accept" />
            </children>
            <padding>
                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
            </padding>
        </HBox>
    </children>
    <stylesheets>
        <URL value="@teoria.css" />
    </stylesheets>
</VBox>
