<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.FlowPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>


<VBox xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUI.Controllers.PopUpController">
   <children>
      <Label alignment="CENTER" prefHeight="21.0" prefWidth="439.0" text="Something Went Wrong :(" />
      <FlowPane prefHeight="200.0" prefWidth="200.0">
         <children>
            <TextArea editable="false" prefHeight="200.0" prefWidth="233.0" fx:id="textArea" />
            <ImageView fitHeight="198.0" fitWidth="206.0" pickOnBounds="true" preserveRatio="true" fx:id="image"/>
            <Pane prefHeight="31.0" prefWidth="278.0" />
            <Button contentDisplay="RIGHT" mnemonicParsing="false" prefHeight="31.0" prefWidth="93.0" text="See more" fx:id="seeMore"/>
            <Button alignment="CENTER_RIGHT" contentDisplay="RIGHT" mnemonicParsing="false" text="Accept" fx:id="accept" onAction="#close"/>
         </children>
      </FlowPane>
   </children>
</VBox>
