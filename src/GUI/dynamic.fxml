<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.geometry.Insets?>

<GridPane alignment="CENTER" hgap="10.0" vgap="10.0"
          xmlns:fx="http://javafx.com/fxml"
          fx:controller="GUI.DynamicController">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <TableView fx:id="tableView" GridPane.columnIndex="0" GridPane.rowIndex="1" editable="true">
        <columns>
            <TableColumn fx:id="variableName" prefWidth="75.0" text="Name" />
            <TableColumn fx:id="variableSize" prefWidth="75.0" text="Type" />
            <TableColumn fx:id="variableValue" prefWidth="75.0" text="Value" />
            <TableColumn fx:id="variableOffset" prefWidth="75.0" text="Offset" />
        </columns>
    </TableView>
</GridPane>
